{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adt-tdmdf"
		},
		"ls_adt_az_fnc_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'ls_adt_az_fnc'"
		},
		"ls_adt_synapse_athena_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_adt_synapse_athena'"
		},
		"ls_adt_synapse_generic_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_adt_synapse_generic'"
		},
		"ls_prod_synapse_athena_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_prod_synapse_athena'"
		},
		"ls_spt_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_spt_datalake'"
		},
		"ls_adt_az_fnc_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://generatefacpac.azurewebsites.net"
		},
		"ls_spt_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sptathenasa.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adt-tdmdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adt_az_fnc_functionKey": {
						"value": "[parameters('ls_adt_az_fnc_functionKey')]"
					},
					"ls_adt_synapse_athena_connectionString": {
						"value": "[parameters('ls_adt_synapse_athena_connectionString')]"
					},
					"ls_adt_synapse_generic_connectionString": {
						"value": "[parameters('ls_adt_synapse_generic_connectionString')]"
					},
					"ls_prod_synapse_athena_connectionString": {
						"value": "[parameters('ls_prod_synapse_athena_connectionString')]"
					},
					"ls_spt_datalake_accountKey": {
						"value": "[parameters('ls_spt_datalake_accountKey')]"
					},
					"ls_adt_az_fnc_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('ls_adt_az_fnc_properties_typeProperties_functionAppUrl')]"
					},
					"ls_spt_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_spt_datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}