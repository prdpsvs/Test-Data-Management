{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tdmadf"
		},
		"ADT_Synapse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADT_Synapse'"
		},
		"Du_fnc_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'Du_fnc'"
		},
		"Prod_Synapse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Prod_Synapse'"
		},
		"ls_staging_storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_staging_storage'"
		},
		"Du_fnc_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://durablefnc.azurewebsites.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tdmadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADT_Synapse_connectionString": {
						"value": "[parameters('ADT_Synapse_connectionString')]"
					},
					"Du_fnc_functionKey": {
						"value": "[parameters('Du_fnc_functionKey')]"
					},
					"Prod_Synapse_connectionString": {
						"value": "[parameters('Prod_Synapse_connectionString')]"
					},
					"ls_staging_storage_connectionString": {
						"value": "[parameters('ls_staging_storage_connectionString')]"
					},
					"Du_fnc_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('Du_fnc_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}